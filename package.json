{
  "name": "node-llm",
  "version": "1.0.0",
  "description": "Node.js LLM integrations",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:tsc",
    "build:clean": "rm -r dist; exit 0",
    "build:tsc": "tsc -p tsconfig.prod.json",
    "command:elasticsearch": "export $(cat .env | xargs) > /dev/null && ts-node src/config/commands/runConfigureElasticsearchCommand.ts",
    "docker:build": "docker build -t node-llm:dev .",
    "docker:run": "docker-compose up node-llm && docker-compose down",
    "docker:clean": "docker-compose down --rmi local --volumes --remove-orphans",
    "docker:start:database": "docker-compose up -d mongo",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "npm:check:updates": "ncu -i --format group",
    "start:dev": "export $(cat .env | xargs) > /dev/null && NODE_ENV=dev ts-node-dev --ignore-watch node_modules ./src/server/start.ts",
    "start:prod": "NODE_ENV=production node dist/src/server/start"
  },
  "author": "Cristian SÃ¡nchez",
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "8.13.1",
    "@langchain/cohere": "0.0.10",
    "@langchain/community": "0.2.4",
    "@langchain/core": "0.2.2",
    "@langchain/mongodb": "0.0.3",
    "@langchain/textsplitters": "0.0.2",
    "awilix": "10.0.2",
    "axios": "1.7.2",
    "cohere-ai": "7.10.1",
    "csv-writer": "1.6.0",
    "d3-dsv": "2.0.0",
    "errorhandler": "1.5.1",
    "express": "4.19.2",
    "express-promise-router": "4.1.1",
    "glob": "10.4.1",
    "http-status": "1.7.4",
    "langchain": "0.2.3",
    "mongoose": "8.4.0",
    "papaparse": "5.4.1",
    "ts-node": "10.9.2",
    "typescript": "5.4.5",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "@types/d3-dsv": "2.0.9",
    "@types/express": "4.17.21",
    "@types/node": "20.12.12",
    "@types/papaparse": "5.3.14",
    "@types/uuid": "9.0.8",
    "eslint-config-codely": "3.1.3",
    "npm-check-updates": "16.14.20",
    "ts-node-dev": "2.0.0",
    "uuid": "9.0.1"
  }
}
